{
  "name": "lesphp/psr4-converter",
  "description": "A tool to facility converting code to PSR-4.",
  "keywords": [
    "PSR-0",
    "PSR-4",
    "namespace",
    "convert"
  ],
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "LesPhp\\PSR4Converter\\": "src/"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "nikic/php-parser": "^4.13",
    "symfony/console": "^6.0",
    "symfony/finder": "^6.0",
    "symfony/serializer": "^6.0",
    "symfony/property-info": "^6.0",
    "symfony/filesystem": "^6.0",
    "lesphp/property-info-typed-array": "^6.0",
    "ramsey/uuid": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  }
}
